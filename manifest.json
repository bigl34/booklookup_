{
  "manifest_version": 2,

  "name": "Booklookup",
  "description": "This extension looks up the book on the Amazon page you are on on libgen.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
    "https://libgen.li/json.php"
    
  ]
}
